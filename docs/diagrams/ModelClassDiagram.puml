@startuml
!include style.puml
skinparam arrowThickness 1.1
skinparam arrowColor MODEL_COLOR
skinparam classBackgroundColor MODEL_COLOR

Package Model <<Rectangle>>{
Interface ReadOnlyOrderBook <<Interface>>
Interface Model <<Interface>>
Interface ObservableList <<Interface>>
Class OrderBook
Class ReadOnlyOrderBook
Class Model
Class ModelManager
Class UserPrefs
Class ReadOnlyUserPrefs

Package Order {
Class Order
Class TransactionId
Class Address
Class Name
Class Phone
Class CashOnDelivery
Class UniqueOrderList
Class Warehouse
Class TimeStamp
}

Package TypeOfItem {
Class TypeOfItem
}

Package Comment {
Class Comment
}
}

Class HiddenOutside #FFFFFF
HiddenOutside ..> Model

OrderBook .up.|> ReadOnlyOrderBook

ModelManager .up.|> Model
Model .right.> ObservableList
ModelManager o--> "1" OrderBook
ModelManager o-left-> "1" UserPrefs
UserPrefs .up.|> ReadOnlyUserPrefs

OrderBook *--> "1" UniqueOrderList
UniqueOrderList o--> "*" Order
Order *--> TransactionId
Order *--> CashOnDelivery
Order *--> Name
Order *--> Phone
Order *--> Address
Order *--> TypeOfItem
Order *--> Warehouse
Order *--> Comment
Order *--> TimeStamp

Name -[hidden]right-> Phone
Phone -[hidden]right-> Address
Address -[hidden]right-> TimeStamp
Order -[hidden]left-> Comment
TimeStamp -[hidden]right-> Warehouse
Warehouse -[hidden]right-> TransactionId
TransactionId -[hidden]right-> CashOnDelivery

ModelManager -->"1" Order : filtered list
@enduml
