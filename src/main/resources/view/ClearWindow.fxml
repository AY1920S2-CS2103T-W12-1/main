<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.stage.*?>

<fx:root resizable="false" title="Warning" type="javafx.stage.Stage" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
    <icons>
        <Image url="@/images/warning_icon.png" />
    </icons>
    <scene>
        <Scene>
         <VBox minWidth="400.0" prefWidth="400.0">
            <children>
                    <Label fx:id="warningMessage" text="Label">
                  <VBox.margin>
                     <Insets />
                  </VBox.margin></Label>
               <HBox alignment="CENTER" spacing="50">
                  <children>
                          <Button fx:id="yesButton" layoutX="50.0" layoutY="50.0" minWidth="50.0" mnemonicParsing="false" onAction="#agreeToClearOrderBookList" prefWidth="50.0" text="Yes">
                        <HBox.margin>
                           <Insets />
                        </HBox.margin></Button>
                          <Button fx:id="noButton" layoutX="50.0" layoutY="50.0" minWidth="50.0" mnemonicParsing="false" onAction="#disagreeToClearOrderBookList" prefWidth="50.0" text="No">
                        <HBox.margin>
                           <Insets />
                        </HBox.margin></Button>
                  </children>
                  <VBox.margin>
                     <Insets top="20.0" />
                  </VBox.margin>
               </HBox>
            </children>
            <opaqueInsets>
               <Insets left="100.0" right="100.0" />
            </opaqueInsets>
            <padding>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
         </VBox>
        </Scene>
    </scene>
</fx:root>
